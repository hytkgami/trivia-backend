{
	"name": "Go & PostgreSQL",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
	"features": {
		"ghcr.io/devcontainers/features/go:1": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"golang.go",
				"GitHub.copilot",
				"GitHub.copilot-labs"
			]
		}
	},
	"forwardPorts": [
		8080,
		5432
	],
	"postCreateCommand": "go version"
}
